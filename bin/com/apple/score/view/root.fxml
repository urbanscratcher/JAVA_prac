<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane 
fx:id="bp" 
prefHeight="668.0" 
prefWidth="850.0" 
xmlns="http://javafx.com/javafx/8" 
xmlns:fx="http://javafx.com/fxml/1" 
fx:controller="com.apple.score.view.TableController"
>
	
	<center>

		<AnchorPane fx:id="ap" prefHeight="668.0" prefWidth="850.0">
			<children>
				<TableView fx:id="tableView" layoutX="35.0" layoutY="115.0" prefHeight="71.0" prefWidth="552.0">
					<columns>
						<TableColumn fx:id="inNumber" prefWidth="75.0" text="번호" />
						<TableColumn fx:id="inClass" prefWidth="120.0" text="클래스" />
						<TableColumn fx:id="inPjName" prefWidth="280.0" text="강의명" />
						<TableColumn fx:id="inDetail" prefWidth="76.0" text="상세보기" />
					</columns>
				</TableView>

				<TableView fx:id="tableDetailView" layoutX="35.0" layoutY="232.0" prefHeight="372.0" prefWidth="800.0" visible="false">
					<columns>
						<TableColumn fx:id="deMemIdC" prefWidth="120.0" text="사용자이름(ID)" />
						<TableColumn fx:id="deAssignNoC" prefWidth="100.0" text="평가항목" />
						<TableColumn fx:id="deAssignNameC" prefWidth="280.0" text="평가명" />
						<TableColumn fx:id="deScoreC" prefWidth="100.0" text="평가점수" />
						<TableColumn fx:id="deGradeC" prefWidth="100.0" text="평가등급" />
						<TableColumn fx:id="deCommentC" prefWidth="100.0" resizable="false" text="코멘트" />
					</columns>
				</TableView>
            <Label layoutX="44.0" layoutY="49.0" text="성적관리" textFill="#3c76aa">
               <font>
                  <Font name="Malgun Gothic" size="30.0" />
               </font></Label>
			</children>
		</AnchorPane>
	</center>
</BorderPane>
