<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane

prefHeight="300" 
prefWidth="300" 
xmlns:fx="http://javafx.com/fxml/1" 
xmlns="http://javafx.com/javafx/8"

>
   <children>
      <TextArea fx:id="commentTa" layoutY="40.0" prefHeight="200.0" prefWidth="300.0" />
      <Label layoutY="0.0" prefHeight="42.0" prefWidth="300.0" text="Comment 입력">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Separator layoutY="40.0" prefHeight="0.0" prefWidth="300.0" />
      <Separator layoutY="239.0" prefHeight="3.0" prefWidth="300.0" />
      <Button fx:id="btnCommentUpdate" layoutY="240.0" mnemonicParsing="false" prefHeight="59.0" prefWidth="148.0" text="저장">
         <font>
            <Font size="29.0" />
         </font>
      </Button>
      <Button fx:id="btnCommentCancel" layoutX="152.0" layoutY="240.0" mnemonicParsing="false" prefHeight="59.0" prefWidth="148.0" text="취소">
         <font>
            <Font size="29.0" />
         </font>
      </Button>
   </children>
	
</AnchorPane>
